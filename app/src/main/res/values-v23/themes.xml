<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.SuperChat" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorBlue</item>
        <item name="colorPrimaryDark">@color/colorBlue</item>
        <item name="colorPrimaryVariant">@color/colorBlue</item>
        <item name="colorOnPrimary">@color/colorBlue</item>
        <item name="colorAccent">@color/colorBlue</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorBlue</item>
        <item name="colorSecondaryVariant">@color/colorBlue</item>
        <item name="colorOnSecondary">@color/colorBlue</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/colorWhiteBg</item>
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/colorWhiteBg</item>
        <item name="android:windowDisablePreview">true</item>

    </style>
</resources>